          "props.data": {
            "binding": {
              "config": {
                "parameters": {
                  "site_id": "{../CurrentSite.props.params.site_id}"
                },
                "queryPath": "CIP_Q/DB_Query/Get_Step_Type",
                "returnFormat": "json"
              },
              "transforms": [
                {
                  "code": "\t\n    # Create a dictionary to store unique names\n    unique_names \u003d {}\n\n    # Loop through each row in the dataset\n    for row in value:\n        name \u003d row[\"name\"]\n        # Check if the name already exists in the dictionary\n        if name not in unique_names:\n            # If not, add it to the dictionary with the entire row as the value\n            unique_names[name] \u003d row\n\n    # Create a list of unique rows from the dictionary\n    unique_rows \u003d list(unique_names.values())\n\n    return unique_rows",
                  "type": "script"
                }
              ],
              "type": "query"
            }
          },
          "props.selection.data[0].id": {
            "onChange": {
              "enabled": null,
              "script": "\tself.getSibling(\"Table_Detail\").refreshBinding(\"props.data\")"
            }
